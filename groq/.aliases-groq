alias bin-groq="cd $HOME/git/setup/groq/bin-groq"
alias tui="cd ~/git/dc-tools/tools/gv-tui"
alias conman="sft ssh conman.c.groq-psw-infra.internal"
alias kc="kubectl"
alias label-nodes="kubectl label nodes -l 'groq.node=true,!topology.groq.io/wiring' topology.groq.io/family=xt topology.groq.io/wiring=xt4444 topology.groq.io/power-pod-id=1 --overwrite"
alias cluster="gv_cli cluster"
alias node="gv_cli node --racks "
alias rack="gv_cli rack --racks "
alias xrk="gv_cli crossrack --racks "
